Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"\[Eta]Hat", "[", 
     RowBox[{"\[Mu]_", ",", "\[Nu]_", ",", "\[Omega]_", ",", "\[Omega]b_"}], 
     "]"}], ":=", 
    RowBox[{
     RowBox[{"MT", "[", 
      RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], "-", 
     FractionBox[
      RowBox[{
       RowBox[{
        RowBox[{"FV", "[", 
         RowBox[{"\[Omega]", ",", "\[Mu]"}], "]"}], 
        RowBox[{"FV", "[", 
         RowBox[{"\[Omega]b", ",", "\[Nu]"}], "]"}]}], "+", 
       RowBox[{
        RowBox[{"FV", "[", 
         RowBox[{"\[Omega]", ",", "\[Nu]"}], "]"}], 
        RowBox[{"FV", "[", 
         RowBox[{"\[Omega]b", ",", "\[Mu]"}], "]"}]}]}], 
      RowBox[{"SP", "[", 
       RowBox[{"\[Omega]", ",", "\[Omega]b"}], "]"}]]}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"graviton", " ", "polarison", " ", "sum"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"SumPol", "[", 
     RowBox[{
     "\[Mu]_", ",", "\[Nu]_", ",", "\[Alpha]_", ",", "\[Beta]_", ",", 
      "\[Omega]_", ",", "\[Omega]b_"}], "]"}], ":=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"\[Eta]Hat", "[", 
         RowBox[{
         "\[Mu]", ",", "\[Alpha]", ",", "\[Omega]", ",", "\[Omega]b"}], "]"}], 
        RowBox[{"\[Eta]Hat", "[", 
         RowBox[{"\[Nu]", ",", "\[Beta]", ",", "\[Omega]", ",", "\[Omega]b"}],
          "]"}]}], "+", 
       RowBox[{
        RowBox[{"\[Eta]Hat", "[", 
         RowBox[{"\[Mu]", ",", "\[Beta]", ",", "\[Omega]", ",", "\[Omega]b"}],
          "]"}], 
        RowBox[{"\[Eta]Hat", "[", 
         RowBox[{
         "\[Nu]", ",", "\[Alpha]", ",", "\[Omega]", ",", "\[Omega]b"}], 
         "]"}]}], "-", 
       RowBox[{
        RowBox[{"\[Eta]Hat", "[", 
         RowBox[{"\[Mu]", ",", "\[Nu]", ",", "\[Omega]", ",", "\[Omega]b"}], 
         "]"}], 
